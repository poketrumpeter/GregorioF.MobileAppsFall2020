<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.movieproduction_finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MainTheme">

        <activity android:name=".FinalResult"></activity>

        <activity
            android:name=".Post_Production"
            android:label="@string/title_activity_post__production"
            android:theme="@style/MainTheme.NoActionBar" />
        <activity

            android:name=".ProductionInfo"
            android:label="@string/title_activity_production_info"
            android:theme="@style/MainTheme.NoActionBar" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>